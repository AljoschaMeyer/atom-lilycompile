{
  "arrow_spacing": {
    "level": "warn"
  },
  "braces_spacing": {
    "level": "warn",
    "spaces": 0,
    "empty_object_spaces": 0
  },
  "camel_case_classes": {
    "level": "warn"
  },
  "colon_assignment_spacing": {
    "level": "warn",
    "spacing": {
      "left": 0,
      "right": 1
    }
  },
  "duplicate_key": {
    "level": "error"
  },
  "ensure_comprehensions": {
    "level": "ignore"
  },
  "indentation": {
    "level": "error",
    "value": 2
  },
  "max_line_length": {
    "level": "warn",
    "max_line_length": 80
  },
  "missing_fat_arrows": {
    "level": "ignore"
  },
  "no_backticks": {
    "level": "warn"
  },
  "no_empty_param_list": {
    "level": "warn"
  },
  "no_interpolation_in_single_quotes": {
    "level": "error"
  },
  "no_stand_alone_at": {
    "level": "warn"
  },
  "no_trailing_semicolons": {
    "level": "warn"
  },
  "no_trailing_whitespace": {
    "level": "warn"
  },
  "no_unnecessary_double_quotes": {
    "level": "warn"
  },
  "prefer_english_operator": {
    "level": "warn"
  },
  "spacing_after_comma": {
    "level": "warn"
  }
}
